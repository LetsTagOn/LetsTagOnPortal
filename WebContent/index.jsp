                                        <input class="form-control input-lg" name="email" id="email" type="email" placeholder="Email address" required>
                                    </div>
                                      <div class="form-group">
                                        <textarea class="form-control" rows="20" name="comments" id="comments"  placeholder="Please leave comments here" required></textarea>
                                    </div>
                                   
                                    <div class="form-group last">
                                        <input type="submit" class="btn btn-warning btn-block btn-lg" id="register" value="Yes, I'am interested!">
